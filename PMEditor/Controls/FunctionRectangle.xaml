<UserControl x:Class="PMEditor.Controls.FunctionRectangle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PMEditor.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid MouseRightButtonUp="Rectangle_MouseRightButtonUp" MouseLeftButtonDown="Rectangle_MouseLeftButtonDown">
        <Border x:Name="highLightBorder" BorderThickness="0">
            <Rectangle Name="rect"/>
        </Border>
    </Grid>
</UserControl>
